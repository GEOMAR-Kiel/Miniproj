ot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="98"/><column index="2" value="205"/><column index="3" value="115"/><column index="4" value="74"/><column index="5" value="98"/><column index="6" value="114"/><column index="7" value="106"/><column index="8" value="300"/><column index="9" value="300"/><column index="10" value="90"/><column index="11" value="97"/><column index="12" value="288"/><column index="13" value="83"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="epsg_primemeridian" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="146"/><column index="2" value="150"/><column index="3" value="143"/><column index="4" value="74"/><column index="5" value="550"/><column index="6" value="300"/><column index="7" value="90"/><column index="8" value="97"/><column index="9" value="174"/><column index="10" value="83"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="epsg_supersession" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="112"/><column index="2" value="196"/><column index="3" value="88"/><column index="4" value="108"/><column index="5" value="131"/><column index="6" value="131"/><column index="7" value="300"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table><table schema="main" name="epsg_unitofmeasure" show_row_id="0" encoding="" plot_x_axis="" unlock_view_pk="_rowid_"><sort/><column_widths><column index="1" value="74"/><column index="2" value="247"/><column index="3" value="100"/><column index="4" value="124"/><column index="5" value="116"/><column index="6" value="131"/><column index="7" value="423"/><column index="8" value="300"/><column index="9" value="90"/><column index="10" value="97"/><column index="11" value="300"/><column index="12" value="83"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Ellipsoids">SELECT
	ellipsoid_code as code,
	ellipsoid_name as name,
	semi_major_axis * uom.factor_b / uom.factor_c as a,
	semi_minor_axis * uom.factor_b / uom.factor_c as b,
	inv_flattening as inv_f,
	ellipsoid.uom_code as uom_code
FROM 
	&quot;epsg_ellipsoid&quot; as ellipsoid 
	JOIN &quot;epsg_unitofmeasure&quot; as uom USING (uom_code);</sql><sql name="ProjectedCRS">SELECT 
	        crs.coord_ref_sys_code AS code,
	        crs.coord_ref_sys_name AS name,
	        datum.ellipsoid_code AS ellipsoid,
	        datum.prime_meridian_code AS primemerid,
	        operation.coord_op_code AS op,
	        operation.coord_op_method_code AS method
        FROM
	        epsg_coordinatereferencesystem as crs 
	        JOIN epsg_coordinatereferencesystem as base_crs
		        ON crs.base_crs_code = base_crs.coord_ref_sys_code
	        JOIN epsg_coordoperation as operation
		        ON operation.coord_op_code = crs.projection_conv_code
	        JOIN (
		        SELECT DISTINCT
			        datum_code,
			        ellipsoid_code,
			        prime_meridian_code
		        FROM
			        epsg_datum
		        WHERE 
			        ellipsoid_code IS NOT NULL AND
			        prime_meridian_code IS NOT NULL
		        UNION SELECT DISTINCT
			        datum_ensemble_code as datum_code,
        			ellipsoid_code,
			        prime_meridian_code
		        FROM
			        epsg_datumensemblemember AS ensemble
			        JOIN epsg_datum
				        ON epsg_datum.datum_code = ensemble.datum_code
		        WHERE
			        epsg_datum.ellipsoid_code IS NOT NULL AND 
			        epsg_datum.prime_meridian_code IS NOT NULL
	        ) AS datum
		        ON datum.datum_code = base_crs.datum_code
        WHERE 
	        crs.coord_ref_sys_kind = 'projected' AND
	        base_crs.coord_ref_sys_kind = 'geographic 2D'</sql><sql name="Datums">SELECT DISTINCT
	datum_code,
	ellipsoid_code,
	prime_meridian_code
FROM
	epsg_datum
WHERE 
	 ellipsoid_code IS NOT NULL AND prime_meridian_code IS NOT NULL
UNION SELECT DISTINCT
	datum_ensemble_code as datum_code,
	ellipsoid_code,
	prime_meridian_code
FROM
	epsg_datumensemblemember AS ensemble
	JOIN epsg_datum ON epsg_datum.datum_code = ensemble.datum_code
WHERE
	epsg_datum.ellipsoid_code IS NOT NULL AND epsg_datum.prime_meridian_code IS NOT NULL</sql><sql name="PrimeMeridians">SELECT
	prime_meridian_code as code,
	prime_meridian_name as name,
	greenwich_longitude * uom.factor_b / uom.factor_c as g_conv,
	greenwich_longitude as g,
	uom.uom_code as uom_code
FROM 
	&quot;epsg_primemeridian&quot; as prime_meridian 
	JOIN &quot;epsg_unitofmeasure&quot; as uom USING (uom_code);</sql><sql name="CheckUOMs">SELECT
	b.unit_of_meas_name,
	c.parameter_name,
	b.remarks,
	a.parameter_value
FROM 
	epsg_coordoperationparamvalue as a 
	JOIN epsg_unitofmeasure as b
		on a.uom_code = b.uom_code and (b.factor_b is null or b.factor_c is null)
	JOIN epsg_coordoperationparam as c
		on c.parameter_code = a.parameter_code
ORDER by b.uom_code</sql><sql name="GetParamValues">SELECT
	val.parameter_code as code,
	val.parameter_value * uom.factor_b / uom.factor_c as v_conv,
	val.parameter_value as v,
	uom.uom_code as uom_code
FROM 
	&quot;epsg_coordoperationparamvalue&quot; as val 
	JOIN &quot;epsg_unitofmeasure&quot; as uom 
		USING (uom_code)
WHERE
	val.coord_op_code = 19986</sql><sql name="GetMethodParamsDescriptions">SELECT
	*
FROM 
	epsg_coordoperationparamusage as usage
	JOIN epsg_coordoperationparam as param on param.parameter_code = usage.parameter_code
WHERE 
	usage.coord_op_method_code = 9820</sql><current_tab id="6"/></tab_sql></sqlb_project>
